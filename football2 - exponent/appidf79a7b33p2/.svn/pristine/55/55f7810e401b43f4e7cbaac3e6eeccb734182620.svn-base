
/*筛选背景color*/
#set{position:relative;top:0;left:0;width:100%;display:none;height:100%;}
#bjcolor-grey{ background:rgb(250,250,250);}
/*筛选共用header*/
#header-live{height:3.8rem; background-color:rgb(11,31,58); padding:0 0.8rem; position:relative;}
.iconfont-filter{ -webkit-font-smoothing: antialiased;-webkit-text-stroke-width: 0.1rem; color: white; width: 3rem; display: block; position:absolute; left:0.8rem;}
.iconfont-filter img {height: 1.6rem;margin-top: 1.5rem;}
#header-live h1{font-size: 1.4rem; color: rgb(255,255,255);line-height:3.8rem; font-weight:400;}

/*筛选*/
.main-filter{ padding-top:3.76rem;}
.main-filter h1,.main-datatype h1{ font-size:1.4rem; color: rgb(102,102,102); font-weight:400; border-bottom:.1rem solid rgb(234,234,234); padding:0 1rem .9rem 1rem;}
.filter-box,.datatype-box{ padding:1.56rem 1rem 3.8rem 1rem; }
/*.filter-box ul{display: flex; justify-content: space-between;justify-content: conter;}*/
.filter-box li,.datatype-box li{ display:block; color:rgb(136,196,37); border:.1rem solid rgb(136,196,37); border-radius:.4rem; width:22%; line-height:2.8rem; height:2.8rem;text-align:center; float:left; margin-right:4%; margin-bottom:1.2rem;}
.filter-box li.fil-hove{color:rgb(255,255,255); border: .1rem solid rgb(238,238,238); background:rgb(136,196,37);}
.datatype-box li.dat-hove{color:rgb(255,255,255); border: .1rem solid rgb(238,238,238); background:rgb(136,196,37);}
.filter-box ul li:nth-child(4n),.datatype-box ul li:nth-child(4n){ margin-right:0;}
.affbuttn-box{ margin:12rem 0  5.8rem 0; padding:0 1rem;}
.affbuttn-box a.aff-buttn{ display:block; background:rgb(136,196,37); width:100%; color:rgb(255,255,255); font-size:1.4rem; height:4.1rem; line-height:4.1rem; text-align:center; border-radius:.4rem;}

/*直播*/
.live-broadcast{ background:rgb(11,31,58); color:rgb(255,255,255); padding:0 1rem;}
.live-bro-box{ height:7.5rem; position:relative;}
.live-bro-time{ position:absolute; left:50%; text-align:center; color:rgb(153,193,248); font-size:.9rem;margin-left:-3.8rem;height: 6.8rem;}
.live-bro-name,.live-bro-name2{ font-size:1.1rem; width:10rem; line-height:5rem;}
.live-bro-name{ position:absolute; left:0; text-align:right;}
.{ height:1.7rem; line-height:1.7rem; color: rgb(153,193,248);}
.live-bro-name2{ position:absolute; right:0; text-align:left;}
.bro-name-half{ padding:0 2rem;margin-bottom: .6rem;}
.bro-name-half span{ display:block; float:left;}
.bro-name-half span:nth-child(2){}
.bro-name-half span:nth-child(3){ margin:0 .3rem;}
.bro-time-list aside:nth-child(2){ margin-left:.3rem;}
.live-bro-time div:nth-child(2){ font-size:1.8rem; line-height:1.8rem; height:1.8rem; color:rgb(255,255,255);}
.live-bro-time .bro-stoped{ width:4.3rem; height:1.4rem; line-height:1.4rem; border-radius:.6rem; background:rgb(221,74,74); color:rgb(255,255,255);    margin: 0 auto;margin-right: 3.2rem; position: absolute; bottom: 0;left: 19%;}
.live-bro-notime div:nth-child(2){font-size: .9rem !important;}
.live-bro-cards{ position:absolute; left:0; bottom:0;}
.live-bro-cards-rl{ position:absolute; right:0; bottom:0;}
.live-bro-bf{ padding:0 2.1rem;}
.live-bro-bf span{ display:block; float:left;}
.live-bro-bf span:nth-child(2){ display:block; float:left; margin:0 .5rem;}
.red-cards img,.yellow-cards img,.cc-cards img{ height:2.1rem;}
.yellow-cards,.cc-cards{ margin-left:1.7rem;}
.red-cards span:nth-child(2),.yellow-cards span:nth-child(2),.cc-cards span:nth-child(2){ margin-left:.7rem;}
.pro-bar-box{ padding:2.9rem 1.2rem 9.5rem 1.2rem;}
.list-pro-gress{ margin-top:.1rem; height:.8rem; background:rgb(242,251,233);}
.list-pro-gress .pro-green { background: #88c425;}
.list-pro-gress span {float: right;width:30px; height:.8rem; line-height: 16px; text-align: right;}
.pro-bar-list td{ font-size:1rem;}
.pro-bar-list td:nth-child(1){ font-size:1.1rem; width:5%; color:rgb(153,153,153); text-align:left;}
.pro-bar-list td:nth-child(2){ width:35%;}
.pro-bar-list td:nth-child(3){ width:20%; text-align:center;}
.pro-bar-list td:nth-child(4){ width:35%;}
.pro-bar-list td:nth-child(5){ font-size:1.1rem; width:5%; color:rgb(153,153,153); text-align:right;}
.pro-bar-list{ margin-bottom:1.4rem;}
.blue-list-pro-gress{ margin-top:.1rem; height:.8rem; background:rgb(229,247,255);}
.blue-list-pro-gress .pro-blue { background:rgb(0,181,254);}
.blue-list-pro-gress span {float:left;width:30px; height:.8rem; line-height: 16px; text-align: right;}
.live-vs-end{ font-size:1.1rem !important;height: 3rem !important;line-height: 3rem !important;padding-right: 1rem;}
/*足球指数*/
.live-scree{ margin-left:1.4rem;}
.live-install a,.live-scree a{ line-height:3.8rem; color:rgb(255,255,255); font-size:1.2rem;}
.live-header{ position:relative; padding:0 2rem !important;}
.live-logo{width: 8rem; position:absolute; left:50%; margin-left:-4rem; padding-top:.5rem;}
.live-logo img{ height:2.7rem;}
.live-tabs{ background:rgb(239,243,244);}
.live-tabs ul{ width:100%;height:2.6rem;Display: flex; justify-content: space-between;}
.live-tabs ul li{ width:33.3%; line-height:2.6rem; text-align:center; color:rgb(89,89,89); font-size:1.1rem;}
.live-tabs ul li.livezs-on{ border-bottom: 0.2rem solid rgb(136,196,37); color: rgb(136,196,37);}
.livezs-data{ line-height:2.8rem; padding:0 1rem; border-bottom: .1rem solid rgb(234,234,234); background:rgb(250,250,250)}
.livezs-data img{ height:1.9rem;}
.livezs-lister{ height:3.1rem; background:rgb(249,252,255);border-bottom:.1rem solid rgb(234,234,234); padding:0 1rem;}
.livezs-lister td{ text-align:left; line-height:3.1rem;font-size:1.1rem; color:rgb(102,102,102);}
.livezs-lister td:nth-child(1){ width:7.5rem;}
.livezs-lister td:nth-child(2){ width:4.8rem; }
.livezs-lister td:nth-child(3){ color:rgb(0,0,0);}
.livezs-lis-tab th{ font-size:1.2rem; height:3.2rem; line-height:3.2rem; font-weight:400;border-bottom:0.1rem solid rgb(234,234,234);}
.livezs-lis-tab td{font-size:1.1rem; height:3.3rem; line-height:3.3rem; text-align:center;border-bottom:0.1rem solid rgb(234,234,234);border-right:.1rem solid rgb(234,234,234); }
.livezs-lis-tab tr td:nth-child(2){ color:rgb(102,102,102);width:6rem;}
.livezs-lis-tab tr:nth-child(2) > td:nth-child(5),.livezs-lis-tab tr:nth-child(3) > td:nth-child(4){ border-right:none;}
.livezs-lis-tab tr:nth-child(4) > td:nth-child(5),.livezs-lis-tab tr:nth-child(5) > td:nth-child(4){ border-right:none;}
.livezs-lis-tab tr:nth-child(3) > td:nth-child(1),.livezs-lis-tab tr:nth-child(5) > td:nth-child(1){ color:rgb(102,102,102)}
/*.livezs-lis-tab tr:nth-child(3) td{ border-bottom:.4rem solid rgb(250,250,250);}*/
.livezs-lis-tab tr:nth-child(2) td:nth-child(1){width:6rem;}
.livezs-lis-tab tr:nth-child(2) > td:nth-child(3),.livezs-lis-tab tr:nth-child(3) > td:nth-child(2){ font-size:1rem; border-right:none;width:6rem;}
.livezs-lis-tab tr:nth-child(4) > td:nth-child(3),.livezs-lis-tab tr:nth-child(5) > td:nth-child(2){ font-size:1rem; color:rgb(221,74,74); border-right:none;}
.livezs-lis-tab tr:nth-child(4) > td:nth-child(4),.livezs-lis-tab tr:nth-child(5) > td:nth-child(3){ font-size:1rem; border-right:none;}
.livezs-lis-tab tr:nth-child(2) > td:nth-child(4),.livezs-lis-tab tr:nth-child(3) > td:nth-child(3){ font-size:1rem; border-right:none;}
.livezs-lis-tab tr:nth-child(2) > td:nth-child(5),.livezs-lis-tab tr:nth-child(3) > td:nth-child(4){ font-size:1rem; width:7rem;}
.livezs-lis-tab tr:nth-child(4) > td:nth-child(5),.livezs-lis-tab tr:nth-child(5) > td:nth-child(4){ font-size:1rem; color:rgb(136,196,37);}
.deepred {color: rgb(221,74,74);}
.live-bf{ margin:0 .3rem;}
.livezs-box margin-bottom:2rem;}
.live-header-tit img {height: 1.7rem !important; margin-top:.9rem !important;}




